ffmpeg -i "input.mkv" -map 0:0 -map 0:1 -map 0:1 -c:v copy -c:a:0 aac -b:a:0 192k -ac 2 -c:a:1 copy "output.conv.mkv"


ffmpeg -i "input.mkv" -map 0:v -map 0:a -map 0:a -c:v copy -c:a:0 aac -b:a:0 192k -ac 2 -c:a:1 copy "output.conv.mkv"

ffmpeg -i "input.mkv" -map 0:v -map 0:a -map 0:a -c:v copy -c:a:0 aac -ac 2 -c:a:1 copy "output.conv.mkv"
